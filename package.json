{
  "name": "bizarre",
  "version": "0.0.1",
  "description": "bizarre package",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest --verbose",
    "build": "tsc -d ./src/index.ts --outDir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KawayAlpaka/bizarre.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KawayAlpaka/bizarre/issues"
  },
  "homepage": "https://github.com/KawayAlpaka/bizarre#readme",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  },
  "files": [
    "dist",
    "lib"
  ],
  "jest": {
    "rootDir": "",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom":[
      "src/**/*.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
